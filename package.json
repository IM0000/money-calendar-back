{
  "name": "backend-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "prebuild:backend": "nest build backend",
    "prebuild:scraping": "nest build scraping",
    "build:backend": "pnpm nest build backend",
    "build:scraping": "pnpm nest build scraping",
    "build": "pnpm build:backend && pnpm build:scraping",
    "format": "pnpm prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:backend": "NODE_ENV=production pnpm nest start backend",
    "start:scraping": "NODE_ENV=production pnpm nest start scraping",
    "start:dev:backend": "pnpm prebuild:backend && NODE_ENV=development pnpm nest start backend --watch",
    "start:dev:scraping": "pnpm prebuild:scraping && NODE_ENV=development pnpm nest start scraping --watch",
    "start:prod:backend": "pnpm build:backend && pnpm node dist/backend/src/main.js",
    "start:prod:scraping": "pnpm build:scraping && pnpm node dist/scraping/src/main.js",
    "lint": "pnpm eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm jest",
    "test:watch": "pnpm jest --watch",
    "test:cov": "pnpm jest --coverage",
    "test:debug": "pnpm node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:backend": "pnpm jest --config ./apps/backend/test/jest-e2e.json",
    "test:e2e:scraping": "pnpm jest --config ./apps/scraping/test/jest-e2e.json",
    "prisma:migrate:dev:backend": "dotenv -e ./apps/backend/src/config/env/.development.env -- prisma migrate dev --name update --schema=./apps/backend/prisma/schema.prisma",
    "prisma:generate:backend:dev": "dotenv -e apps/backend/src/config/env/.development.env -- npx prisma generate --schema=apps/backend/prisma/schema.prisma",
    "prisma:generate:backend:prod": "dotenv -e apps/backend/src/config/env/.production.env -- npx prisma generate --schema=apps/backend/prisma/schema.prisma",
    "prisma:generate:scraping:dev": "dotenv -e apps/scraping/src/config/env/.development.env -- npx prisma generate --schema=apps/scraping/prisma/schema.prisma",
    "prisma:generate:scraping:prod": "dotenv -e apps/scraping/src/config/env/.production.env -- npx prisma generate --schema=apps/scraping/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.6.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cheerio": "1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "form-data": "^4.0.0",
    "https-proxy-agent": "^7.0.5",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-discord": "^0.1.4",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "passport-oauth2": "^1.8.0",
    "qs": "^6.13.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.16",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/qs": "^6.9.15",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "prisma": "^6.6.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "Transform": "AWS::Serverless-2016-10-31"
}
